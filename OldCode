import React, {Component} from 'react';
import {Router} from '../routes';

class register extends Component {
    constructor (props) {
        super(props);
        this.state = {
            isPasswordVisible: false,
            password: '',
            username: '',
            fname: '',
            lname: '',
            street: '',
            houseNr: '',
            postCode: '',
            PlaceOfRes: '',
            DateOfBirth: '',
            nat: '',
            email: '',
            mobNr: '',
            IDfront: '',
            IDback: '',
            mrz: '',
        }
    }

    render() {
        return (
